{
  "extends": ["airbnb", "plugin:react/recommended"],
  "env": {
    "browser": true
  },
  "rules": {
    "quote-props": ["error", "as-needed"],
    "indent": ["error", 4],
    "react/jsx-indent-props": ["error", 4],
    "react/jsx-indent": ["error", 4],
    "no-trailing-spaces": ["error", { "skipBlankLines": true }],
    "react/prefer-stateless-function": 0,
    "react/jsx-no-bind": 0
  }
}
